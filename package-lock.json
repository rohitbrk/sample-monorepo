{
  "name": "testapp",
  "version": "1.0.0",
  "lockfileVersion": 3,
  "requires": true,
  "packages": {
    "": {
      "name": "testapp",
      "version": "1.0.0",
      "license": "ISC",
      "workspaces": [
        "apps/*",
        "packages/*"
      ]
    },
    "apps/app-a": {
      "name": "@testapp/app-a",
      "version": "1.0.0",
      "license": "ISC",
      "dependencies": {
        "@testapp/package-b": "*"
      }
    },
    "apps/app-b": {
      "name": "@testapp/app-b",
      "version": "1.0.0",
      "license": "ISC",
      "dependencies": {
        "@testapp/package-a": "*"
      }
    },
    "node_modules/@testapp/app-a": {
      "resolved": "apps/app-a",
      "link": true
    },
    "node_modules/@testapp/app-b": {
      "resolved": "apps/app-b",
      "link": true
    },
    "node_modules/@testapp/package-a": {
      "resolved": "packages/package-a",
      "link": true
    },
    "node_modules/@testapp/package-b": {
      "resolved": "packages/package-b",
      "link": true
    },
    "packages/package-a": {
      "name": "@testapp/package-a",
      "version": "1.0.0",
      "license": "ISC"
    },
    "packages/package-b": {
      "name": "@testapp/package-b",
      "version": "1.0.0",
      "license": "ISC"
    }
  }
}
